{
  "name": "mhs-11",
  "version": "1.0.0",
  "description": "MHS blog on eleventy",
  "main": "index.js",
  "scripts": {
    "prestart": "run-s clean",
    "start": "run-p develop:*",
    "prebuild": "run-p clean",
    "build": "run-s build:*",
    "build:generate": "run-s 11ty:build css:build",
    "develop:generate": "run-p 11ty:watch css:watch",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "11ty:build": "eleventy --quiet --output dist",
    "11ty:watch": "eleventy --quiet --serve --output dist",
    "css:watch": "postcss src/**/*.css --base src/assets/css --dir dist/css --watch --verbose",
    "css:build": "postcss src/**/*.css --base src/assets/css --dir dist/css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "body-parser": "^1.19.0",
    "browserslist": "^4.14.7",
    "caniuse-lite": "^1.0.30001157",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "fast-glob": "^2.2.7",
    "global": "^4.4.0",
    "graphql": "^15.4.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-implicit-figures": "^0.9.0",
    "markdown-it-link-attributes": "^2.1.0",
    "markdown-it-toc-done-right": "^3.0.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "nunjucks-date-filter": "^0.1.1",
    "postcss-cli": "^8.2.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^2.7.1",
    "serverless-http": "^2.6.0",
    "theo": "^8.1.5"
  },
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "stylelint": "^13.7.2",
    "stylelint-order": "^2.2.1"
  }
}
